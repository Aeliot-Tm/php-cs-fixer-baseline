{
    "alias": "pcsf-baseline.phar",
    "base-path": "/app/",
    "directories": [
        "src"
    ],
    "files": [
        "composer.json",
        "composer.lock",
        "LICENSE",
        "README.md"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                ".github",
                "doc",
                "test",
                "test_old",
                "tests",
                "Tests",
                "vendor-bin"
            ],
            "in": "vendor"
        }
    ],
    "main": "bin/pcsf-baseline",
    "output": "pcsf-baseline.phar"
}
